<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - kruce blake</title>
    <meta name="description" content="Learn about Kruce Blake, a software engineer with professional experience since 2011 and a passion for all things related to software development.">
    @{
        var request = Context.Request;
        var ogImage = $"{request.Scheme}://{request.Host.Value}{Url.Content("~/dist/images/kruce.jpg")}";
    }
    <meta property="og:image" content="@ogImage" />
    <meta property="og:image:width" content="200" />
    <meta property="og:image:height" content="200" />
    <meta property="og:image:alt" content="A profile photograph of Kruce Blake on a sunny wedding day." />
    <link rel="icon" type="image/png" href="~/favicon-96x96.png" sizes="96x96" />
    <link rel="icon" type="image/svg+xml" href="~/favicon.svg" />
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link rel="apple-touch-icon" sizes="180x180" href="~/apple-touch-icon.png" />
    <meta name="apple-mobile-web-app-title" content="Kruce Blake" />
    <link rel="manifest" href="~/site.webmanifest" />
    <link rel="preconnect" href="https://fonts.googleapis.com" crossorigin="anonymous" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="anonymous" />
    <link href="https://fonts.googleapis.com/css2?family=Varela+Round&display=swap" rel="stylesheet" crossorigin="anonymous" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet" crossorigin="anonymous" />
    <environment include="Development">
        <link rel="stylesheet" href="~/dist/lib/bootstrap/css/bootstrap.css" />
        <link rel="stylesheet" href="~/dist/lib/fontawesome-free/css/all.css" />
        <link rel="stylesheet" href="~/dist/lib/aos/aos.css" />
        <link rel="stylesheet" href="~/source/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous"
              asp-fallback-href="~/dist/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="hidden"
              asp-fallback-test-property="display"
              asp-fallback-test-value="none"
              asp-suppress-fallback-integrity="true" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" integrity="sha384-0c38nfCMzF8w8DBI+9nTWzApOpr1z0WuyswL4y6x/2ZTtmj/Ki5TedKeUcFusC/k" crossorigin="anonymous"
              asp-fallback-href="~/dist/lib/fontawesome-free/css/all.min.css"
              asp-fallback-test-class="fa"
              asp-fallback-test-property="display"
              asp-fallback-test-value="inline-block"
              asp-suppress-fallback-integrity="true" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css" integrity="sha384-/rJKQnzOkEo+daG0jMjU1IwwY9unxt1NBw3Ef2fmOJ3PW/TfAg2KXVoWwMZQZtw9" crossorigin="anonymous"
              asp-fallback-href="~/dist/lib/aos/aos.css"
              asp-fallback-test-class="[data-aos^=fade][data-aos^=fade].aos-animate"
              asp-fallback-test-property="opacity"
              asp-fallback-test-value="1"
              asp-suppress-fallback-integrity="true">
        <link rel="stylesheet" href="~/dist/css/site.min.css" asp-append-version="true" />
    </environment>
</head>
<body id="page-top">
    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
        <div class="container">
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                Menu
                <i class="fas fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse justify-content-center" id="navbarResponsive">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#about" aria-label="Scroll down to about section">about</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#projects" aria-label="Scroll down to projects section">projects</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#contact-info" aria-label="Scroll down to about contact">contact</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- Header -->
    <header class="masthead">
        <video id="backgroundVideo" class="background-video" autoplay loop muted poster="~/dist/images/stars_full.png">
            <environment include="Development">
                <source src="~/dist/images/stars.mp4" type="video/mp4" aria-label="slowly zooming in then out on space with a bunch of similar shaped stars lit up blue by a brighter centered star in a much further distance">
                <source src="~/dist/images/stars.webm" type="video/webm" aria-label="slowly zooming in then out on space with a bunch of similar shaped stars lit up blue by a brighter centered star in a much further distance">
            </environment>
            <environment exclude="Development">
                <source src="https://cdn.kruceblake.com/stars.mp4" type="video/mp4" aria-label="slowly zooming in then out on space with a bunch of similar shaped stars lit up blue by a brighter centered star in a much further distance">
                <source src="https://cdn.kruceblake.com/stars.webm" type="video/webm" aria-label="slowly zooming in then out on space with a bunch of similar shaped stars lit up blue by a brighter centered star in a much further distance">
            </environment>
            <img src="~/dist/images/stars_full.png" title="Your browser does not support the <video> tag" alt="space with a bunch of similar shaped stars lit up blue by a brighter centered star in a much further distance">
        </video>
        <div class="container d-flex h-100 align-items-center">
            <div id="headerContent" class="mx-auto text-center">
                <canvas id="mastHeadCanvas"></canvas>
                <h1 class="mx-auto my-0">kruce blake</h1>
                <h2 class="text-white-50 mx-auto mt-2 mb-5">
                    <a href="" class="text-white-50 typewrite" data-period="2000" data-type='[ "software engineer", "application developer", "programmer", "full-stack developer"  ]' aria-label="rotating text in the style of a typewriter typing it that reads: software engineer, application developer, programmer, and full-stack developer">
                        <span class="wrap"></span>
                    </a>
                </h2>
                <div id="scrollAnimation" class="scroll-animation">
                    <a class="nav-link js-scroll-trigger" href="#about" aria-label="Scroll down to about section"><span></span></a>
                </div>
            </div>
        </div>
    </header>
    @RenderBody()
    <div id="eggBox" class="egg"></div>
    <!-- Footer -->
    <footer class="bg-black small text-center text-white-50">
        <div class="container">
            kruceblake.com &copy; @DateTime.Now.Year
        </div>
    </footer>
    <environment include="Development">
        <script src="~/dist/lib/jquery/jquery.js"></script>
        <script src="~/dist/lib/bootstrap/js/bootstrap.bundle.js"></script>
        <script src="~/dist/lib/jquery-easing/jquery.easing.js"></script>
        <script src="~/dist/lib/aos/aos.js"></script>
        <script src="~/source/js/grayscale.js"></script>
        <script src="~/source/js/typewriter.js"></script>
        <script src="~/source/js/cheet.js"></script>
        <script src="~/source/js/video-pause.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js" integrity="sha384-ZvpUoO/+PpLXR1lu4jmpXWu80pZlYUAfxl5NsBMWOEPSjUn/6Z/hRTt8+pR6L4N2" crossorigin="anonymous"
                asp-fallback-src="~/dist/lib/jquery/jquery.min.js"
                asp-fallback-test="window.jQuery"
                asp-suppress-fallback-integrity="true">
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/js/bootstrap.bundle.min.js" integrity="sha384-xrRywqdh3PHs8keKZN+8zzc5TX0GRTLCcmivcbNJWm2rs5C8PRhcEn3czEjhAO9o" crossorigin="anonymous"
                asp-fallback-src="~/dist/lib/bootstrap/js/bootstrap.bundle.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                asp-suppress-fallback-integrity="true"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js" integrity="sha384-leGYpHE9Tc4N9OwRd98xg6YFpB9shlc/RkilpFi0ljr3QD4tFoFptZvgnnzzwG4Q" crossorigin="anonymous"
                asp-fallback-src="~/dist/lib/jquery-easing/jquery.easing.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.easing"
                asp-suppress-fallback-integrity="true"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js" integrity="sha384-n1AULnKdMJlK1oQCLNDL9qZsDgXtH6jRYFCpBtWFc+a9Yve0KSoMn575rk755NJZ" crossorigin="anonymous"
                asp-fallback-src="~/dist/lib/aos/aos.js"
                asp-fallback-test="window.AOS"
                asp-suppress-fallback-integrity="true"></script>
        <script src="~/dist/js/site.min.js" asp-append-version="true"></script>
    </environment>
    <script>AOS.init();</script> @*initialize animate on scroll*@
    @RenderSection("Scripts", required: false)
</body>
</html>